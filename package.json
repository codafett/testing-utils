{
  "name": "@tiney/testing-utils",
  "version": "1.1.6",
  "description": "Testing utils",
  "author": "Reuben Greaves <reuben@tiney.co>",
  "license": "ISC",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tiney/tiney-services.git"
  },
  "scripts": {
    "transpile": "tsc && tscpaths -p tsconfig.json -s ./src -o ./lib/src",
    "transpile:clean": "npm run clean:lib && npm run transpile",
    "clean:lib": "rm -rf lib",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run transpile:clean",
    "lint": "eslint --ext .ts,.js,.json",
    "lint:ci": "npm run lint -- --format junit --output-file ~/reports/eslint.xml",
    "lint:package-json": "npmPkgJsonLint -q"
  },
  "dependencies": {
    "express": "4.17.1",
    "jest": "29.5.0",
    "jest-junit": "16.0.0",
    "jest-runner-groups": "2.2.0",
    "nock": "13.3.0",
    "supertest": "6.3.3",
    "typedi": "0.10.0",
    "typeorm": "0.3.15",
    "typeorm-transactional-tests": "2.0.0"
  },
  "devDependencies": {
    "@types/express": "4.17.17",
    "@types/jest": "25.2.2",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "eslint": "8.38.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-ejs": "0.0.2",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-no-only-tests": "3.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "husky": "8.0.3",
    "lint-staged": "13.0.0",
    "npm-package-json-lint": "5.1.0",
    "prettier": "2.8.7",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "tscpaths": "0.0.9",
    "typescript": "4.7.4"
  },
  "lint-staged": {
    "*.{js,ts,json}": [
      "prettier --config .prettierrc.js --write",
      "eslint --ext .ts,.js,.json"
    ],
    "package.json": [
      "npm run lint:package-json"
    ]
  }
}
