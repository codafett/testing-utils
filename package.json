{
  "name": "@tiney/testing-utils",
  "version": "0.0.1",
  "description": "Testing utils",
  "author": "Reuben Greaves <reuben@tiney.co>",
  "license": "ISC",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tiney/tiney-services.git"
  },
  "scripts": {
    "transpile": "tsc && tscpaths -p tsconfig.json -s ./src -o ./lib/src",
    "transpile:clean": "npm run clean:lib && npm run transpile",
    "clean:lib": "rm -rf lib",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run transpile:clean"
  },
  "dependencies": {
    "@tiney/infrastructure": "latest",
    "express": "4.17.1",
    "supertest": "6.3.3",
    "typeorm": "0.2.45",
    "typeorm-transactional-tests": "1.1.5",
    "jest": "29.1.0",
    "jest-junit": "10.0.0",
    "jest-runner-groups": "1.1.1"
  },
  "devDependencies": {
    "@types/jest": "25.2.2",
    "tscpaths": "^0.0.9",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  }
}
